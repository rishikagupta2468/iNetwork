<!-- Original area -->
<div class="about-area area-padding">
  <div class="container-fluid">
    <div class="section-headline text-center">
      <h2>All Posts</h2>
    </div>
    <div id="feeds" class="row text-center">
      <% feeds.forEach(function(feed){%>
      <div class="col-md-3 col-sm-6 col-xs-12">
        <div class="thumbnail all">
          <img src="<%= feed.image %>" alt="<%= feed.name%>" />
          <div class="caption">
            <h4><%= feed.name %></h4>
            <p><%= feed.description.substring(0,100) %></p>
            <p>
              <a class="btn btn-large center-block btn-primary arrow" href="/feeds/<%= feed._id %>">More Info</a>
            </p>
          </div>
        </div>
      </div>
      <%}); %>
    </div>
  </div>
</div>
